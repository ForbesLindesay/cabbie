{
  "plugins": [
    "syntax-flow",
    "syntax-trailing-function-commas",
    "syntax-class-properties",
    "./babel-plugin-flow-enum",

    ["add-header-comment", {
      "header": [
        "@flow",
        "This file is generated automatically, run npm run build to re-generate."
      ]
    }]
  ],
  "env": {
    "sync": {
      "plugins": [
        "./babel-plugin-remove-async",
        [
          "module-alias",
          [
            {
              "src": "./src/auto-request/sync",
              "expose": "auto-request",
            }
          ]
        ]
      ]
    },
    "async": {
      "plugins": [
        [
          "module-alias",
          [
            {
              "src": "./src/auto-request/async",
              "expose": "auto-request",
            }
          ]
        ]
      ]
    }
  }
}
