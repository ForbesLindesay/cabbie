{
  "name": "cabbie",
  "private": true,
  "version": "0.11.8",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "description": "A webdriver client",
  "keywords": [
    "cabbie",
    "webdriver"
  ],
  "dependencies": {
    "@types/node": "^7.0.31",
    "available-browsers": "^1.0.3",
    "cross-spawn": "^5.0.1",
    "depd": "^1.1.0",
    "dotenv": "^4.0.0",
    "taxi-rank": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-live": "^4.0.1",
    "babel-plugin-add-header-comment": "^1.0.3",
    "babel-plugin-flow-runtime": "0.6.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-flow-comments": "^6.21.0",
    "babel-preset-forbeslindesay": "^2.1.0",
    "babel-runtime": "^6.20.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.16.0",
    "browserify-middleware": "^7.1.0",
    "chalk": "^1.1.3",
    "chromedriver": "^2.21.2",
    "concurrently": "^3.5.0",
    "deep-equal": "^1.0.1",
    "envify": "^4.0.0",
    "express": "^4.14.0",
    "flow-bin": "^0.40.0",
    "flow-runtime": "0.6.0",
    "highlight.js": "^9.9.0",
    "lsr": "^1.0.0",
    "markdown-it": "^8.2.2",
    "mkdirp": "^0.5.1",
    "ms": "^0.7.2",
    "pluralize": "^3.1.0",
    "prettier": "0.19.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "4.0.0-alpha.6",
    "resolve": "^1.2.0",
    "rimraf": "^2.5.4",
    "s3": "^4.4.0",
    "stop": "^3.1.0",
    "styled-components": "^2.0.0",
    "sync-request": "^4.1.0",
    "then-request": "^2.0.1",
    "thread-sleep": "^2.0.0",
    "throat": "^3.0.0",
    "typescript": "^2.3.4"
  },
  "scripts": {
    "prettier": "node scripts/prettier",
    "prewww": "npm run prettier",
    "www": "babel-node www",
    "www:publish": "NODE_ENV=production babel-node www/publish",
    "prebuild": "npm run prettier",
    "build": "babel-node scripts/build",
    "pretest": "babel-node scripts/build --only-tests",
    "test": "babel-node test/sync/src && babel-node test/async/src",
    "test:record": "npm run pretest && babel-node test/async/src --record",
    "test:sauce": "npm run pretest && babel-node test/sync/src --sauce && babel-node test/async/src --sauce",
    "release": "npm run build && npm run publish:all",
    "publish:all": "concurrently -n \"async,sync,core\" \"npm run publish:async\" \"npm run publish:sync\" \"npm run publish:core\"",
    "publish:async": "cd output/async && npm publish",
    "publish:sync": "cd output/sync && npm publish",
    "publish:core": "cd core && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ForbesLindesay/cabbie.git"
  },
  "author": "ForbesLindesay",
  "contributors": [
