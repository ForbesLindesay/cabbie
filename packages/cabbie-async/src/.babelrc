{
  "plugins": [
    "syntax-flow",
    "syntax-trailing-function-commas",
    "syntax-class-properties",
    "syntax-object-rest-spread",
    "../babel-plugin-flow-enum",

    ["add-header-comment", {
      "header": [
        "@flow",
        "This file is generated automatically, run npm run build to re-generate."
      ]
    }]
  ],
  "env": {
    "sync": {
      "plugins": [
        "../babel-plugin-remove-async",
      ]
    },
  }
}
