{
  "name": "cabbie-monorepo",
  "private": true,
  "bolt": {
    "workspaces": [
      "packages/*"
    ]
  },
  "version": "0.12.1",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "description": "A webdriver client",
  "keywords": [
    "cabbie",
    "webdriver"
  ],
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@types/babel__core": "^7.1.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/deep-equal": "^1.0.1",
    "@types/depd": "^1.1.32",
    "@types/dotenv": "^6.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/ms": "^0.7.30",
    "@types/node": "^10.1.2",
    "assert-never": "^1.1.0",
    "available-browsers": "^1.0.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-live": "^4.0.1",
    "babel-plugin-add-header-comment": "^1.0.3",
    "babel-plugin-flow-runtime": "0.6.0",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-flow-comments": "^6.21.0",
    "babel-preset-forbeslindesay": "^2.1.0",
    "babel-runtime": "^6.20.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.16.0",
    "bolt": "^0.22.6",
    "browserify-middleware": "^7.1.0",
    "chalk": "^2.4.2",
    "chromedriver": "^2.46.0",
    "concurrently": "^3.5.0",
    "cross-spawn": "^6.0.5",
    "deep-equal": "^1.0.1",
    "depd": "^2.0.0",
    "dotenv": "^7.0.0",
    "envify": "^4.0.0",
    "express": "^4.14.0",
    "flow-bin": "^0.95.0",
    "flow-runtime": "0.6.0",
    "flowgen2": "^2.2.0",
    "highlight.js": "^9.9.0",
    "lsr": "^2.0.0",
    "markdown-it": "^8.2.2",
    "mkdirp": "^0.5.1",
    "ms": "^0.7.2",
    "pluralize": "^3.1.0",
    "prettier": "^1.16.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "4.0.0-alpha.6",
    "resolve": "^1.2.0",
    "rimraf": "^2.5.4",
    "s3": "^4.4.0",
    "stop": "^3.1.0",
    "styled-components": "^2.0.0",
    "sync-request": "^6.1.0",
    "taxi-rank": "^2.1.0",
    "then-request": "^6.0.0",
    "thread-sleep": "^2.2.0",
    "throat": "^3.0.0",
    "typescript": "^3.3.4000"
  },
  "scripts": {
    "prettier": "prettier --write 'packages/*/{src,scripts}/**/*.{ts,tsx,js,jsx,json,yml,md}' '*.{ts,tsx,js,jsx,json,yml,md}' '.circleci/config.yml'",
    "prewww": "npm run prettier",
    "www": "babel-node www",
    "www:publish": "NODE_ENV=production babel-node www/publish",
    "prebuild": "npm run prettier",
    "build": "bolt ws run build",
    "postbuild": "mkdirp tarballs && cd tarballs && npm pack ./../packages/*",
    "pretest": "babel-node scripts/build --only-tests",
    "test": "babel-node test/sync/src && babel-node test/async/src",
    "test:record": "npm run pretest && babel-node test/async/src --record",
    "test:sauce": "npm run pretest && babel-node test/sync/src --sauce && babel-node test/async/src --sauce",
    "release": "npm run build && npm run publish:all",
    "publish:all": "concurrently -n \"async,sync,core\" \"npm run publish:async\" \"npm run publish:sync\" \"npm run publish:core\"",
    "publish:async": "cd output/async && npm publish",
    "publish:sync": "cd output/sync && npm publish",
    "publish:core": "cd core && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ForbesLindesay/cabbie.git"
  },
  "author": "ForbesLindesay",
  "contributors": [
    {
      "name": "Marcel Erz",
      "email": "marcel.erz@gmail.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "ts-node": "^8.0.3"
  }
}
